<ul class="album-sidebar-list">
  <p>Add to Album</p>
  <% albums.each(function(album) { %>
    <li data-album-id="<%= album.id %>" class="group">
      <img src="<%= album.escape("image_url") %>"
           class="album-sidebar-photo"/>

      <p class="album-sidebar-title">
        <%= album.escape("title") %>
      </p>
    </li>

  <%  }) %>
</ul>
